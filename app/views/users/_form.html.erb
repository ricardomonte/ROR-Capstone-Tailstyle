<%= simple_form_for @user, url: users_path do |f| %>
  <%= f.error_notification %>
  <%= f.input :username %>
  <%= f.input :name %>
  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.button :submit, class: "btn-primary" %>
  <%= f.button :button, "Cancel", type: "reset", class: "btn-outline-secondary" %>
<% end %>